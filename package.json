{
    "name": "@ascentcore/dataspot",
    "version": "1.0.0",
    "description": "Data Science Package for Javascript",
    "scripts": {
        "test": "jest --coverage --passWithNoTests",
        "lint": "npm run lint:json && npm run lint:ts",
        "lint:json": "prettier --list-different .eslintrc *.json",
        "lint:ts": "eslint --ext .js,.ts --ignore-path .gitignore .",
        "build": "tsc",
        "generate-docs": "rimraf docs && typedoc --options ./typedoc.json . --plugin typedoc-plugin-markdown"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ascentcore/dataspot.git"
    },
    "keywords": [
        "data",
        "science",
        "clustering"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ascentcore/dataspot/issues"
    },
    "homepage": "https://github.com/ascentcore/dataspot#readme",
    "devDependencies": {
        "@types/d3": "^5.16.3",
        "@types/jsdom": "^16.2.4",
        "@types/node-fetch": "^2.5.7",
        "@types/pouchdb": "^6.4.0",
        "@types/sharp": "^0.26.0",
        "@typescript-eslint/eslint-plugin": "^2.2.0",
        "@typescript-eslint/parser": "^2.2.0",
        "eslint": "^6.1.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.1.0",
        "jest": "^26.4.2",
        "prettier": "^1.18.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.3.0",
        "typedoc": "^0.19.1",
        "typedoc-plugin-markdown": "^2.4.2",
        "typescript": "^3.8.0"
    },
    "dependencies": {
        "canvas": "^2.6.1",
        "d3": "^6.2.0",
        "jsdom": "^16.4.0",
        "node-fetch": "^2.6.1",
        "pouchdb": "^7.2.2",
        "sharp": "^0.26.2"
    }
}
